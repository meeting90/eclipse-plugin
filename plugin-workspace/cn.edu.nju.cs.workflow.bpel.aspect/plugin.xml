<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="cn.edu.nju.cs.workflow.bpel.aspect.editors.AspectMultipageEditPart"
            contributorClass="cn.edu.nju.cs.workflow.bpel.aspect.editors.AspectEditorContributor"
            extensions="aspect"
            icon="icons/sample.gif"
            id="cn.edu.nju.cs.workflow.bpel.aspect.editors.AspectMultipageEditPart"
            name="aspect editor">
         <contentTypeBinding
               contentTypeId="cn.edu.nju.cs.workflow.bpel.aspect.contenttype">
         </contentTypeBinding>
      </editor>
   </extension>
   <extension
         point="org.eclipse.core.contenttype.contentTypes">
      <content-type
            base-type="org.eclipse.core.runtime.xml"
            default-charset="UTF-8"
            file-extensions="aspect"
            id="cn.edu.nju.cs.workflow.bpel.aspect.contenttype"
            name="BPEL aspect File"
            priority="normal">
         <describer
               class="cn.edu.nju.cs.workflow.bpel.aspect.editors.AspectContentDescriber">
         </describer>
      </content-type>
   </extension>
   <extension
         point="org.eclipse.emf.ecore.extension_parser">
      <parser
            class="cn.edu.nju.cs.workflow.bpel.aspect.resource.AspectResourceFactoryImpl"
            type="aspect">
      </parser>
   </extension>
   <extension
         point="org.eclipse.emf.ecore.content_parser">
      <parser
            class="cn.edu.nju.cs.workflow.bpel.aspect.resource.AspectResourceFactoryImpl"
            contentTypeIdentifier="cn.edu.nju.cs.workflow.bpel.aspect.contenttype">
      </parser>
   </extension>
   <extension
         point="org.eclipse.emf.ecore.generated_package">
      <package
            class="cn.edu.nju.cs.workflow.model.aspect.AspectPackage"
            genModel="src/model/aspect.genmodel"
            uri="http://cn.edu.nju.cs.workflow.bpel.aspect">
      </package>
   </extension>

</plugin>
