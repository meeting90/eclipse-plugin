<!-- Templete BPEL Process [Generated by the Eclipse BPEL Designer] -->
<!-- Date: ${date} -->
<bpel:process name="Templete"
         targetNamespace="cs.nju.edu.cn/workflow/example"
         suppressJoinFailure="yes"
         xmlns:tns="cs.nju.edu.cn/workflow/example"
         xmlns:ns="$partner_namespace"
         xmlns:bpel="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
         >

    <!-- Import the client WSDL -->
	<bpel:import location="TempleteArtifacts.wsdl" namespace="cs.nju.edu.cn/workflow/example" 
	        importType="http://schemas.xmlsoap.org/wsdl/" />
	<bpel:import location="${partner_wsdl}" namespace=""$partner_namespace" 
	        importType="http://schemas.xmlsoap.org/wsdl/" />
         
    <!-- ================================================================= -->         
    <!-- PARTNERLINKS                                                      -->
    <!-- List of services participating in this BPEL process               -->
    <!-- ================================================================= -->         
    <bpel:partnerLinks>
        <!-- The 'client' role represents the requester of this service. -->
        <bpel:partnerLink name="client"
                     partnerLinkType="tns:Templete"
                     myRole="TempleteProvider"
                     />
        <bpel:partnerLink name="${partnerLink}"
                     partnerLinkType="ns:${partnerLinkType}"
                     partnerRole="${PartnerProvider}"
                     />
    </bpel:partnerLinks>
  
    <!-- ================================================================= -->         
    <!-- VARIABLES                                                         -->
    <!-- List of messages and XML documents used within this BPEL process  -->
    <!-- ================================================================= -->         
    <bpel:variables>
        <!-- Reference to the message passed as input during initiation -->
        <bpel:variable name="input"
                  messageType="tns:TempleteRequestMessage"/>
        <bpel:variable name="output"
                  messageType="tns:TempleteResponseMessage"/>
        <bpel:variable name="${invoke_name}_input"
                  messageType="ns:${requestMessage}"/>
        <bpel:variable name="${invoke_name}_output"
                  messageType="ns:${responseMessage}"/>
    </bpel:variables>

    <!-- ================================================================= -->         
    <!-- ORCHESTRATION LOGIC                                               -->
    <!-- Set of activities coordinating the flow of messages across the    -->
    <!-- services integrated within this business process                  -->
    <!-- ================================================================= -->         
    <bpel:sequence name="main">
        
        <!-- Receive input from requester. 
             Note: This maps to operation defined in Templete.wsdl 
             -->
        <bpel:receive name="receiveInput" partnerLink="client"
                 portType="tns:Templete"
                 operation="process" variable="input"
                 createInstance="yes"/>
        <bpel:invoke name="${invoke_name}" partnerLink="${invoke_name}_PL" 
        operation=${operation} portType=ns:${port_type} 
        inputVariable=${invoke_name}_input outputVariable=${invoke_name}_output>
        </bpel:invoke>
        <!-- Generate reply to synchronous request -->
        <bpel:reply name="replyOutput" 
               partnerLink="client"
               portType="tns:Templete"
               operation="process" 
               variable="output"
               />
    </bpel:sequence>
</bpel:process>

