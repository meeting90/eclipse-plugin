module syntax 

language Calculang

test Add [[
  1 + 2
]] parse succeeds

test Multiply [[
  1 * 2
]]

test Abstract syntax (1) [[
  1
]]
  parse to Int("1")

test Abstract syntax (2) [[
  1 * 2
]]
  parse to Mul(Int("1"), Int("2"))

test Parentheses [[
  (1 + 2)
]]

test Multiply and add [[
  1 + 2 * 3
]] parse to [[
  1 + (2 * 3)
]]

test Add and multiply [[
  1 * 2 + 3
]] parse to [[
  (1 * 2) + 3
]]

test Add and add [[
  1 + 2 + 3
]] parse to [[
  (1 + 2) + 3
]]
